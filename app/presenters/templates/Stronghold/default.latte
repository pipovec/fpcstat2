<div class="row" style="padding-bottom:10; margin-top:50px;">
<hr />
</div>
<div class="row">
  <div class="col-sm-6">
      <div class="well">
        <canvas id="SkirmishTier6"></canvas>
          <script>
            var ctx = document.getElementById("SkirmishTier6");
            var label = {php echo  json_encode($data6batL) };
            var dats = {php echo  json_encode($data6batD) };

            var myChart = new Chart(ctx, {
                
                type: 'bar',
                data: {
                  
                    labels: label,        
                    datasets: [{                       
                        label: 'Pocet bitiek',
                        data: dats,
                        backgroundColor: 'rgba(255, 0, 0, 0.3)',
                        borderWidth: 2
                        
                    }]
                }, 
                options: {
                    title: {
                      display: true,          
                      text: 'Najpoužívanejšie tanky v ŠARVÁTKACH tier 6 za včerajšok'
                    }
                }    
            });
          </script>		  
      </div>
  </div>

