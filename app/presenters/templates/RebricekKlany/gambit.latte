                    $("#dvesto").append("<tr><td>"+value["award_level"]+".</td><td>"+value["battle_fame_points"]+"</td><td>"+value["name"]+"</td>"
                    +"<td>"+value["rank"]+"</td><td>"+value["rank_delta"]+"</td><td>"+value["tag"]+"</td><td>"+value["task_fame_points"]+"</td>"
                    +"<td>"+value["total_fame_points"]+"</td><td>"+ d.toLocaleDateString()+"  "+ d.toLocaleTimeString()+"</td></tr>" ); 
                }
            });
           
                } 
            });
         


         
            $.get({
            url: "https://api.worldoftanks.eu/wot/globalmap/eventrating/?application_id=883ff6ceefb13177357ffea34d6fb06f&event_id=event_gambit&front_id=gambit&page_no=3&limit=100",
            type: "POST",            
                success: function (result) {                              
               
                
               
                $.each(result["data"],function(key,value){
                
                if(Search(value["clan_id"])) {
                    var d =  new Date(value["updated_at"]*1000);                     
                    
                    $("#tristo").append("<tr><td>"+value["award_level"]+".</td><td>"+value["battle_fame_points"]+"</td><td>"+value["name"]+"</td>"
                    +"<td>"+value["rank"]+"</td><td>"+value["rank_delta"]+"</td><td>"+value["tag"]+"</td><td>"+value["task_fame_points"]+"</td>"
                    +"<td>"+value["total_fame_points"]+"</td><td>"+ d.toLocaleDateString()+"  "+ d.toLocaleTimeString()+"</td></tr>" ); 
                }
            });
           
                } 
            });
         
            
         
            $.get({
            url: "https://api.worldoftanks.eu/wot/globalmap/eventrating/?application_id=883ff6ceefb13177357ffea34d6fb06f&event_id=event_gambit&front_id=gambit&page_no=4&limit=100",
            type: "POST",            
                success: function (result) {                              
               
                
               
                $.each(result["data"],function(key,value){
                
                if(Search(value["clan_id"])) {
                    var d =  new Date(value["updated_at"]*1000);                     
                    
                    $("#styristo").append("<tr><td>"+value["award_level"]+".</td><td>"+value["battle_fame_points"]+"</td><td>"+value["name"]+"</td>"
                    +"<td>"+value["rank"]+"</td><td>"+value["rank_delta"]+"</td><td>"+value["tag"]+"</td><td>"+value["task_fame_points"]+"</td>"
                    +"<td>"+value["total_fame_points"]+"</td><td>"+ d.toLocaleDateString()+"  "+ d.toLocaleTimeString()+"</td></tr>" ); 
                }
            });
           
                } 
            });

            $.get({
            url: "https://api.worldoftanks.eu/wot/globalmap/eventrating/?application_id=883ff6ceefb13177357ffea34d6fb06f&event_id=event_gambit&front_id=gambit&page_no=5&limit=100",
            type: "POST",            
                success: function (result) {                              
               
                
               
                $.each(result["data"],function(key,value){
                
                if(Search(value["clan_id"])) {
                    var d =  new Date(value["updated_at"]*1000);                     
                    
                    $("#patsto").append("<tr><td>"+value["award_level"]+".</td><td>"+value["battle_fame_points"]+"</td><td>"+value["name"]+"</td>"
                    +"<td>"+value["rank"]+"</td><td>"+value["rank_delta"]+"</td><td>"+value["tag"]+"</td><td>"+value["task_fame_points"]+"</td>"
                    +"<td>"+value["total_fame_points"]+"</td><td>"+ d.toLocaleDateString()+"  "+ d.toLocaleTimeString()+"</td></tr>" ); 
                }
            });
           
                } 
            });
         
            
            
            
        });
    </script>
{/block}
