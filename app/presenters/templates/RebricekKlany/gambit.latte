{block head}
<title>CZ-SK ulicka slavy</title>
<meta name="Keywords" content="WOT,OPERATION GAMBIT,GAMBIT,CZ/SK Alley of fame, kampan, world of tanks">
<meta name="Description" content="Ulicka slavy ceskych a slovenskych klanov v kampani Gambit">
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300"> 
{/block}


{block content}
<div class="col-sm-12" style="margin-top:30px;">
	<h4 class="page-header"> Rebríčky Českých a Slovenských klanov v kampani GAMBIT</h4>
   
</div>
<div class="row">
    <div class="col-sm-1" style="width:150px"><p></p></div>
    <div class="col-sm-11 table-responsive" style="font-family: Roboto; text-align: center;">
            <table class="table-striped text-center">
                <thead>
                <tr>
                    <th style="text-align: center;">Award level</th>
                    <th style="text-align: center;">Battle fame point</th>                                
                    <th style="text-align: center;">Name</th>
                    <th style="text-align: center;">Rank</th>
                    <th style="text-align: center;">Rank delta</th>
                    <th style="text-align: center;">Tag</th>
                    <th style="text-align: center;">task_fame_points</th>
                    <th style="text-align: center;">total_fame_points</th>
                    <th style="text-align: center;">updated_at</th>
                </thead>
                <tbody id="sto"></tbody>
                <tbody id="dvesto"></tbody>
                <tbody id="tristo"></tbody>
                <tbody id="styristo"></tbody>
                <tbody id="patsto"></tbody>                        
            </table>
    </div>            
</div>

<script >

        $(document).ready(function(){
            
            var clans = [];
            $.ajax({
                type:"GET",
                url: {link kampan!},
                dataType: "json",

                success: function(data) {
                   $.each(data , function(key,value){
                       clans[key] = value["clan_id"];                       
                   });
                }
            });
            
            
            function Search(integer) {
                var count = clans.length;
                for(var i = 0; i < count; i ++) {
                    if(clans[i] === integer) { return true; }
                }
                return false;
            }
           
            
            $.get({
            url: "https://api.worldoftanks.eu/wot/globalmap/eventrating/?application_id=883ff6ceefb13177357ffea34d6fb06f&event_id=event_gambit&front_id=gambit&page_no=1&limit=100",
            type: "POST",            
                success: function (result) {                              
               
               
                $.each(result["data"],function(key,value){
                
                if(Search(value["clan_id"])) {
                    var d =  new Date(value["updated_at"]*1000);                     
                    
                    $("#sto").append("<tr><td>"+value["award_level"]+".</td><td>"+value["battle_fame_points"]+"</td><td>"+value["name"]+"</td>"
                    +"<td>"+value["rank"]+"</td><td>"+value["rank_delta"]+"</td><td>"+value["tag"]+"</td><td>"+value["task_fame_points"]+"</td>"
                    +"<td>"+value["total_fame_points"]+"</td><td>"+ d.toLocaleDateString()+"  "+ d.toLocaleTimeString() +"</td></tr>" ); 
                }
            });
           
                } 
            });

         
            $.get({
            url: "https://api.worldoftanks.eu/wot/globalmap/eventrating/?application_id=883ff6ceefb13177357ffea34d6fb06f&event_id=event_gambit&front_id=gambit&page_no=2&limit=100",
            type: "POST",            
                success: function (result) {                              
                
               
               
                $.each(result["data"],function(key,value){
                
                if(Search(value["clan_id"])) {
