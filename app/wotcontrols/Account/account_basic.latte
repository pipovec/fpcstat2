{block content}
<div class="row">
    <div class="row col-sm-2  table-responsive">
        <table>
            <tr><td><strong>{$info['nickname']} [{$info['clan_id']}]</strong></td></tr> 
            <tr><td>Global rating: </td><td  class="text-right">{$info['global_rating']} </td></tr>
            <tr><td>Client language:</td><td  class="text-right"> {$info['client_language']}</td></tr>
            <tr><td>WN8:</td><td class="text-right" style="background-color:{$wn8['color']|noescape};color: black;">{$wn8['wn8']}</td></tr>
        </table>
    </div>

    <div class="row col-sm-6 small table-responsive">
    <table class="table table-bordered table-striped ">
        <thead>
            <tr style="col-sm-6">
                <th class="text-center">Učet vytvorený</th>
                <th class="text-center">Posledná bitka</th>
                <th class="text-center">Posledna aktualizácia</th>
                <th class="text-center">Odhlásený</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td class="text-center">{$info['created_at']|date:'%d.%m.%Y %H:%M:%S'} </td>
                <td class="text-center">{$info['last_battle_time']|date:'%d.%m.%Y %H:%M:%S'}</td>
                <td class="text-center">{$info['updated_at']|date:'%d.%m.%Y %H:%M:%S'} </td>
                <td class="text-center">{$info['logout_at']|date:'%d.%m.%Y %H:%M:%S'}</td>
            </tr>
        </tbody>
    </table>
    </div>
</div>

<div class="row">
    
    <div class="col-sm-8 small table-responsive">
    
    <table class="table table-bordered table-striped">
        <tr>
            <th ></th>
            <th class="text-center" >Bitiek</th>
            <th class="text-center">Víťazstiev</th>
            <th class="text-center">Prehier</th>
            <th class="text-center">Remíz</th>
            <th class="text-center">Prežitých</th>
            <th class="text-center">Frags</th>
            <th class="text-center">DMG</th>
            <th class="text-center">Výstrelov</th>
            <th class="text-center">Percento zásahov</th>
            <th class="text-center">CP</th>
            <th class="text-center">DCP</th>
            <th class="text-center">priemerne XP za bitku</th>
        </tr>

        <tr>
            <th >Celkom</th>
            <td class="text-center">{$all['battles']}</td>
            <td class="text-center">{$all['wins']/$all['battles']*100|number:3} %</td>
            <td class="text-center">{$all['losses']/$all['battles']*100|number:2} %</td>
            <td class="text-center">{$all['draws']/$all['battles']*100|number:2} %</td>
            <td class="text-center">{$all['survived_battles']/$all['battles']*100|number:2} %</td>
            <td class="text-center">{$all['frags']/($all['battles'] - $all['survived_battles'] )|number:2}</td>
            <td class="text-center">{$all['damage_dealt']/$all['damage_received']|number:2}</td>
            <td class="text-center">{$all['shots']}</td>
            <td class="text-center">{$all['hits_percents']} %</td>
            <td class="text-center">{$all['capture_points']}</td>
            <td class="text-center">{$all['dropped_capture_points']}</td>
            <td class="text-center">{$all['xp']/ $all['battles']|number:0}</td>

        </tr>

        <tr>
            <th >v CW starych</th>
            <td class="text-center">{$clan['battles']|number:0}</td>
            <td class="text-center">{$clan['wins']/$clan['battles']*100|number:3} %</td>
            <td class="text-center">{$clan['losses']/$clan['battles']*100|number:2} %</td>
            <td class="text-center">{$clan['draws']/$clan['battles']*100|number:2} %</td>
            <td class="text-center">{$clan['survived_battles']/$clan['battles']*100|number:2} %</td>
            <td class="text-center">{$clan['frags']/($clan['battles'] - $clan['survived_battles'] )|number:2}</td>
            <td class="text-center">{$clan['damage_dealt']/$clan['damage_received']|number:2}</td>
            <td class="text-center">{$clan['shots']}</td>
            <td class="text-center">{$clan['hits_percents']} %</td>
            <td class="text-center">{$clan['capture_points']}</td>
            <td class="text-center">{$clan['dropped_capture_points']}</td>
            <td class="text-center">{$clan['xp']/ $clan['battles']|number:0}</td>

        </tr>

        <tr>
